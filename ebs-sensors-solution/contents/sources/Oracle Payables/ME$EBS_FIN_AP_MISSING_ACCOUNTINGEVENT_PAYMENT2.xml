    <DisplayName>EBS Sensor: FIN AP Approved Payments Distributions Missing ACCOUNTING EVENT ID - Weekly</DisplayName>
    <Description><![CDATA[Under some circumstances some approved invoice distributions and/or payments are missing accounting_event_id's.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_AP_MISSING_ACCOUNTINGEVENT_PAYMENT2' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'check_id:' || s.check_id || ', invoice_payment_id:' || s.invoice_payment_id key_data FROM (SELECT /*+ ordered use_nl(ac, aip) */      ac.check_id            check_id,      aip.invoice_payment_id invoice_payment_id  FROM      (          SELECT DISTINCT              ph.check_id          FROM              apps.ap_payment_history_all ph          WHERE              ph.accounting_date BETWEEN ( trunc(sysdate) - 7 ) AND ( trunc(sysdate) + 3 )      )                       chks,      apps.ap_checks_all           ac,      apps.ap_invoice_payments_all aip  WHERE          ac.check_id = aip.check_id      AND ( ( aip.accounting_event_id IS NULL              AND nvl(aip.posted_flag, 'N') <> 'Y' )            OR EXISTS (          SELECT              1          FROM              apps.ap_payment_history_all aph          WHERE                  aph.check_id = ac.check_id              AND aph.accounting_event_id IS NULL              AND nvl(aph.posted_flag, 'N') <> 'Y'      ) )      AND ac.check_id = chks.check_id      AND ROWNUM < 11  ORDER BY      check_id) s]]>
        </SQLQuery>
        <IsInclude>1</IsInclude>
            <InUse>1</InUse>
            <DBQueryFieldList>
                <DBPatternSeqField>entrytime</DBPatternSeqField>
                <DBPatternField>
                    <DBQueryColName>sensor</DBQueryColName>
                    <FieldName>udfs1</FieldName>
                </DBPatternField>
                <DBPatternField>
                    <DBQueryColName>entrytime</DBQueryColName>
                    <FieldName>time</FieldName>
                </DBPatternField>
                <DBPatternField>
