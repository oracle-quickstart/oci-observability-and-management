    <DisplayName>EBS Sensor: FIN AP Quantity Billed or Amount Billed Mismatch - Weekly</DisplayName>
    <Description><![CDATA[Canceled and discarded invoice lines mismatch where the quantity invoiced has not been set to 0.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_AP_NOT_NULL_QTY_INV' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'invoice_id:' || s.invoice_id key_data FROM (SELECT ail.invoice_id invoice_id       FROM apps.ap_invoice_lines_all ail,            ( SELECT DISTINCT invoice_id FROM apps.ap_invoice_distributions_all WHERE accounting_date BETWEEN (trunc(sysdate) - 7)  AND (trunc(sysdate) + 3) ) invs       WHERE (nvl(discarded_flag,'N') = 'Y' OR              nvl(cancelled_flag,'N') = 'Y')       AND   po_line_location_id is not null       AND   nvl(quantity_invoiced,0) <> 0       AND   ail.invoice_id = invs.invoice_id  and rownum < 11  order by invoice_id) s]]>
        </SQLQuery>
        <IsInclude>1</IsInclude>
            <InUse>1</InUse>
            <DBQueryFieldList>
                <DBPatternSeqField>entrytime</DBPatternSeqField>
                <DBPatternField>
                    <DBQueryColName>sensor</DBQueryColName>
                    <FieldName>udfs1</FieldName>
                </DBPatternField>
                <DBPatternField>
                    <DBQueryColName>entrytime</DBQueryColName>
                    <FieldName>time</FieldName>
                </DBPatternField>
                <DBPatternField>
