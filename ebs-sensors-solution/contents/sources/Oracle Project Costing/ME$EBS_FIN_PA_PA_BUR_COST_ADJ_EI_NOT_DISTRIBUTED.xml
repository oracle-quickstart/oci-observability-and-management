    <DisplayName>EBS Sensor: FIN PA PRC Distribute Total Burdened Cost process errors with ADJ EI NOT DISTRIBUTED - Weekly</DisplayName>
    <Description><![CDATA[PRC: Distribute Total Burdened Cost Failed with error "Adjusted expenditure not distributed".]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_PA_PA_BUR_COST_ADJ_EI_NOT_DISTRIBUTED' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'project_number:' || s.project_number || ', expenditure_item_id:' || s.expenditure_item_id || ', ind_cost_dist_rejection_code:' || s.ind_cost_dist_rejection_code || ', adjusted_expenditure_item_id:' || s.adjusted_expenditure_item_id key_data FROM (SELECT DISTINCT         p.segment1 project_number,         expenditure_item_id expenditure_item_id,         ind_cost_dist_rejection_code ind_cost_dist_rejection_code,         adjusted_expenditure_item_id adjusted_expenditure_item_id  FROM apps.pa_expenditure_items_all ei,       apps.pa_projects_all p  WHERE ei.project_id = p.project_id  AND   ei.org_id = p.org_id  AND   ei.ind_cost_dist_rejection_code like 'ADJ_EI_NOT_DISTRIBUTED'  AND   ei.adjusted_expenditure_item_id is not null  AND   NOT EXISTS (          SELECT 1           FROM apps.pa_cost_distribution_lines_all cdl          WHERE cdl.expenditure_item_id = ei.expenditure_item_id          AND   cdl.line_type IN ('C','D'))  AND   ROWNUM <= 6) s]]>
        </SQLQuery>
