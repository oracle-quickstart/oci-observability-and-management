    <DisplayName>EBS Sensor: MFG OM Order Lines in BOOKED status that are missing Workflow Assignment Activity - Weekly</DisplayName>
    <Description><![CDATA[One order has been returned but more could exist. Need to create the workflow activity that is missing.]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_MFG_OM_MISSING_WF' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'order_number:' || s.order_number key_data FROM (select order_number order_number  from  (SELECT h.order_number,  TO_DATE (sysdate-7,'DD-MON-YYYY') startdate  from apps.oe_order_headers_all h  where h.open_flag = 'Y'  and nvl(h.cancelled_flag, 'N') = 'N'  and not exists (select 'WF is pending' from apps.wf_items wi  where to_number(wi.item_key) = h.header_id  and wi.item_type = 'OEOH'  and wi.end_date is null)  and nvl(h.transaction_phase_code, 'F') = 'F'  and rownum < 2)) s]]>
        </SQLQuery>
        <IsInclude>1</IsInclude>
            <InUse>1</InUse>
            <DBQueryFieldList>
                <DBPatternSeqField>entrytime</DBPatternSeqField>
                <DBPatternField>
                    <DBQueryColName>sensor</DBQueryColName>
                    <FieldName>udfs1</FieldName>
                </DBPatternField>
                <DBPatternField>
                    <DBQueryColName>entrytime</DBQueryColName>
                    <FieldName>time</FieldName>
                </DBPatternField>
                <DBPatternField>
                                        <DBQueryColName>key_data</DBQueryColName>
                                        <FieldName>data</FieldName>
                                   </DBPatternField>
            </DBQueryFieldList>
            <IsSystem>0</IsSystem>
            <IsEnabled>1</IsEnabled>
            </DBPattern>
    </Patterns>
