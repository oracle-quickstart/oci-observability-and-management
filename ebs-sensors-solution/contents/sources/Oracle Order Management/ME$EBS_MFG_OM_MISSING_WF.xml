    <Source name="EBS Sensor: MFG OM Order Lines in BOOKED status that are missing Workflow Assignment Activity - Weekly" oms_version="2.50.2.0.0" configWarningSettings="0">
    <SourceType>database_sql</SourceType>
    <TargetTypes>
