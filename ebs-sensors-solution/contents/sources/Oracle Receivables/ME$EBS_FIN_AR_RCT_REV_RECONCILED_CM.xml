    <Source name="EBS Sensor: FIN AR Reversed Receipt is Reconciled with a Bank Statement Line - Weekly" oms_version="2.50.2.0.0" configWarningSettings="0">
    <SourceType>database_sql</SourceType>
    <TargetTypes>
